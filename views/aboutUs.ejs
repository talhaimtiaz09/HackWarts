        <script>
          let navDropDown=document.querySelector('.nav-drop-down')
          let profileIcon=document.querySelector('.profileIcon')
          profileIcon.addEventListener('click',()=>{
            const currHeight=getComputedStyle(navDropDown).maxHeight
            if(currHeight=='0px')
